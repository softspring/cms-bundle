{% trans_default_domain 'sfs_cms_contents' %}

{% for entity in entities %}
    <tr>
        <td class="p-3">{{ entity.name }}</td>
        <td class="p-3 text-end">
            <a href="{{ url('sfs_cms_admin_content_'~content~'_details', {'content':entity}) }}">{{ ('admin_'~content~'.list.actions.details.link')|trans }}</a>
        </td>
    </tr>
{% endfor %}