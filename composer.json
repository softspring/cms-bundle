{
    "name": "softspring/cms-bundle",
    "description": "A cms bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/collections": "^1.8 | ^2.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/migrations": "^3.2",
        "doctrine/orm": "^2.14",
        "sensio/framework-extra-bundle": "^5.4|^6.2",
        "softspring/components": "^5.1",
        "softspring/crudl-controller": "^5.1",
        "softspring/doctrine-migrations-version-comparator": "^5.1",
        "softspring/doctrine-target-entity-resolver": "^5.1",
        "softspring/dynamic-form-type": "^5.1",
        "softspring/events": "^5.1",
        "softspring/media-bundle": "^5.1",
        "softspring/permissions-bundle": "^5.1",
        "softspring/polymorphic-form-type": "^5.1",
        "softspring/twig-extra-bundle": "^5.1",
        "symfony/config": "^5.4|^6.0",
        "symfony/intl": "^5.4|^6.0",
        "symfony/routing": "^5.4|^6.0",
        "symfony/uid": "^5.4|^6.0",
        "symfony/validator": "^6.2",
        "symfony/yaml": "^5.4|^6.0",
        "twig/twig": "^2.10|^3.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.17",
        "google/cloud-storage": "^1.30",
        "javihgil/doctrine-pagination-bundle": "^2.0",
        "phpstan/phpstan": "^1.10.22",
        "phpunit/phpunit": "^10.2.0",
        "softspring/user-bundle": "^5.1",
        "symfony/maker-bundle": "^1.38",
        "symfony/web-profiler-bundle": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CmsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CmsBundle\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev"
        }
    }
}
